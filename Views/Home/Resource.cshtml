@using System.Collections.Generic
@using Digitalisert.Models
@model IEnumerable<ResourceModel.Resource>

@{
    ViewData["Title"] = "Resource Page";
}

<partial name="~/Views/Shared/_Navbar.cshtml" />

@if (Model.Any(r => r.Properties.Any(p => p.Tags.Contains("@wkt"))))
{
  <partial name="~/Views/Shared/_Map.cshtml" />
}

@if (Model.Skip(1).Any())
{
  <partial name="~/Views/Shared/_ListGroup.cshtml" />
}
else if (Model.Any())
{
  <partial name="~/Views/Shared/_Resource.cshtml" model="Model.First()"/>
}
